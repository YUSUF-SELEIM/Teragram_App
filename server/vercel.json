{
    "version": 2,
    "builds": [
        {
            "src": "dest/app.js",
            "use": "@vercel/node",
            "config":{
                "includeFiles": ["dest/**"] 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dest/app.js"
        }
    ]
}