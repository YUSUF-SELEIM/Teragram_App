{
    "version": 2,
    "builds": [
        {
            "src": "dest/app.js",
            "use": "@vercel/node",
            "config":{
                "includeFiles": ["dest/**"] 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dest/app.js"
        },
        {
            "src":"/graphql","dest":"dest/app.js"   
        },
        {
            "src":"/api/set-cookie","dest":"dest/app.js"   
        }
    ]
}